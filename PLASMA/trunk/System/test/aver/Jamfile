SubDir PLASMA System test aver ;

if ! $(PLASMA_READY) {
  SubInclude PLASMA System test aver converter ;

  local modtest = modtest ;

  AverToCpp $(modtest:S=.cc) : $(modtest) ;

  AverMainNoNddl aver-module-tests : modtest : module-tests.cc : System ;
  RunModuleMain run-aver-module-tests : aver-module-tests ;
  LocalDepends tests : run-aver-module-tests ;
}
