SubDir PLASMA System documentation ;

if ! $(PLASMA_READY) {

rule Latex 
{
  local file = $(1) ;
  local target = $(file:S=.tex) ;
}

actions Latex {
  latex $(1) ;
  bibtex $(1) ;
  latex $(1) ;
  latex $(1) ;
  dvipdf $(1) ;
}

Latex user-guide : user-guide.tex ;

#ModuleDocumentation System ;

} # PLASMA_READY
