class Location{
 string name;
 Location(string _name){
  name = _name;
 }
}

class Path{
 Location from, to;
 Path(Location _from, Location _to){
  from = _from;
  to = _to;
 }
}

class World{
 Location rock;
 Location hill;
 Location lander;
 Location martianCity;

 Path p1;
 Path p2;
 Path p3;
 Path p4;

 predicate Going{
  Location from;
  Location to;
  neq(from, to);
 }

 predicate initialState{}
 World(){
  rock = new Location("ROCK");
  hill = new Location("HILL");
  lander = new Location("LANDER");
  martianCity = new Location("MARTIAN_CITY");
  p1 = new Path(rock, hill);
  p2 = new Path(lander, hill);
  p3 = new Path(martianCity, hill);
  p4 = new Path(hill, lander);
 }
}

World::Going{
 Path path : {
  eq(path.from, from);
  eq(path.to, to);
 };

 Path p1: {};
}

World::initialState{
 any(Going g);
 eq(g.from, object.rock);
 eq(g.to, object.hill);

 Path p1 : {
  eq(p1.from, g.from);
  eq(p1.to, g.to);
 };

 Path p2 : {
  eq(p2.from, g.from);
  eq(p2.to, g.to);
 };

 Path p3 : {};
}

World world = new World();
close();
goal(World.initialState g0);
g0.activate();
