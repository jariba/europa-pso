// Use enumerations to declare user-defined collections where the elements are primitves
enum Ints {1, 2, 3, 4}; // Enumeration of integers - numeric with no decimal
enum Floats {1.2, 2.3, 3.4, 67.9}; // Enumeration of floats - decimal point used
enum Strings {'s1', 's2', 's3'}; // Enumeration of strings - single quoted values
enum Symbols {s4, s5, s6}; // Enumeration of symbols
enum Fruits {Apple, Orange, Banana}; // Enumeration of symbols
enum Speed {Slow, Medium, Fast}; // Enumeration of symbols

// Allocate Variables of primitive type and eneumerated version of same type
int a;
Ints b;
float c;
Floats d;
string e = 's1';
Strings f;
Symbols g; // Cannot allocate a symbol singleton! No primitive for it.

// Post equality constraints demonstrating compatibility
eq(a, b);
eq(c, d);
eq(e, f);
eq(g, s4);
