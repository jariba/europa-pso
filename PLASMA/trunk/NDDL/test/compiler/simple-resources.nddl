#include "../../core/Resources.nddl"

class Battery extends Resource {}

class World{
 Battery mainBattery;
 Battery reserveBattery;

 predicate initialState{}

 World(){
  mainBattery = new Battery(0, 100, 100, 0, 1000);
  reserveBattery = new Battery(0, 100, 100, 100, 1000);
 }
}

World::initialState{
 any(Battery.change tx);
 eq(tx.quantity, 4);
}
