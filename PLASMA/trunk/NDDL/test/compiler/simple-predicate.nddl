// Simple class and predicates
enum Locations {Hill, Rock, Lander};

class Foo{
 int m_0;

 predicate pred1{
  Locations m_0;
  int m_1;
  float m_2;
  string m_3;
  bool m_4;
  World m_5;
 }
}

// Declares mandatory world class, with constructor with no arguments.
class World {
 Foo m_foo1;
 predicate initialState{
  any(Foo.pred1 a);
 }

 World(){
  m_foo1 = new Foo();
 }
}