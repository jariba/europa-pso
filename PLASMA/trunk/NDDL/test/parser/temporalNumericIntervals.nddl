class tempNumIntervals {
    predicate Go {
        {
            before [4 5] (Stop);
            after [2 1] (Stop);
            contains [2 3] [3 4] (tempNumIntervals.Stop);
        }
    }
    predicate Stop {
        {
            any (Stop s);
            this before [4 5] s;
            s after [2 1] this;
            this contains [2 3] [3 4] s;
        }
    }
}
