	vartype_decl.anml var_obj_declaration.anml objtype_decl.anml objtype_body_stmt.anml
	function_declaration.anml
	CrewPlanning-problem-instance.anml
{
  RunModuleMain run-anml-tests : ANMLParser : $(model) $(model:S=.dot) ;
}

} # PLASMA_READY
