class DNP_Data___SSR_Storage_Use extends Timeline {
  predicate Record {
    int Start_Storage;
    int End_Storage;
    int Length;
    Observation_Level ReqFPRate;
    Observation_Level ReqMosRate;
    REC_Past Rec_Prior;
    REC_Future Rec_Post;
    Record_Obs What_obs;
    int DSS_Opp_Num;
  }

  predicate Playback {
    int Start_Storage;
    int End_Storage;
    int Playback_Rate;
    DSS_Id Locaton;
    int DSS_Opp_Num;
    int Allowed_Earlier_Opps;
    End_Flag Flagged_End;
    PB_Past PB_Prior;
  }

  predicate Constant_Storage {
    int SSR_Storage;
    int DSS_Opp_Num;
    End_Flag Flagged_End;
    CS_Past CS_Prior;
    CS_Future CS_Post;
  }

  DNP_Data __DDL__object;
  DNP_Data___SSR_Storage_Use(DNP_Data object) {
    __DDL__object = object;
  }
}

class DNP_Data {
  DNP_Data___SSR_Storage_Use SSR_Storage_Use;
